{
  "name": "esbuild-plugin-denolint",
  "version": "1.1.0",
  "description": "Lint your esbuild bundles with denolint.",
  "author": "Ferdinand Prantl <prantlf@gmail.com> (http://prantl.tk)",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/esbuild-plugin-denolint/blob/master/LICENSE"
    }
  ],
  "homepage": "http://github.com/prantlf/esbuild-plugin-denolint/",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/esbuild-plugin-denolint.git"
  },
  "bugs": {
    "url": "http://github.com/prantlf/esbuild-plugin-denolint/issues"
  },
  "type": "module",
  "module": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "rollup -c",
    "lint": "denolint && tsc --noEmit test/types.ts",
    "check": "teru test/index.cjs && teru-esm test/*.js",
    "cover": "c8 teru-esm test/*.js",
    "test": "denolint && tsc --noEmit test/types.ts && teru test/index.cjs && c8 teru-esm test/*.js"
  },
  "c8": {
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "libdenolint": "^2.0.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.8.3",
    "c8": "^7.12.0",
    "denolint": "^2.0.2",
    "esbuild": "^0.15.10",
    "esbuild-plugin-denolint": "link:",
    "rollup": "^2.79.1",
    "tehanu": "^1.0.1",
    "tehanu-repo-coco": "^1.0.0",
    "tehanu-teru": "^1.0.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "esbuild": "^0"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "esbuild-plugin",
    "eslint",
    "denolint",
    "deno-lint",
    "deno",
    "lint"
  ]
}
